{
  "name": "jira-release-actions",
  "version": "0.0.0",
  "private": true,
  "description": "Jira Release/Archive action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justin-JHG/jira-release-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Justin Liu",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@typescript-eslint/parser": "^5.37.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.28.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.3"
  }
}
