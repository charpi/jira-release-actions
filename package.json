{
  "name": "jira-release-actions",
  "version": "0.0.0",
  "private": true,
  "description": "Jira Release/Archive action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --newLine lf",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --no-cache --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justin-JHG/jira-release-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Justin Liu",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.2",
    "@typescript-eslint/parser": "^5.61.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.44.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^29.6.1",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
